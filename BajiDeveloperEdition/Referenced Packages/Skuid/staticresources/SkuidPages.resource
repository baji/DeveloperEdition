s.push(nf);\n                    }\n        \n                    // Add in a basic model\n                    newLayout += '&lt;model id=\"' + modelId + '\" limit=\"100\" query=\"true\" '\n                        + 'createrowifnonefound=\"false\" sobject=\"' + sobject + '\"&gt;';\n        \n                    // Add in the object's name field(s)\n                    // and the CreatedDate field\n                    newLayout += '&lt;fields&gt;';\n                    $.each(nameFields,function(i,f){\n                        newLayout +=    '&lt;field id=\"' + f +'\"/&gt;';\n                    });\n                    newLayout +=        '&lt;field id=\"CreatedDate\"/&gt;';\n                    newLayout += '&lt;/fields&gt;';\n                    \n                    // Close out the Model and start the Components section\n                    newLayout += '&lt;conditions/&gt;&lt;/model&gt;&lt;/models&gt;&lt;components&gt;';\n                \n                    //\n                    // DESKTOP TAB PAGE\n                    //\n        \n                    if (pageType==='Desktop'){\n                        // Create a Page Title component\n                        newLayout += '&lt;pagetitle model=\"' + modelId + '\"&gt;'\n                            + '&lt;maintitle&gt;&lt;template&gt;{{Model.labelPlural}}&lt;/template&gt;&lt;/maintitle&gt;'\n                            + '&lt;subtitle&gt;&lt;template&gt;Home&lt;/template&gt;&lt;/subtitle&gt;'\n                            + '&lt;actions/&gt;&lt;/pagetitle&gt;';\n            \n                        // Create a Table component\n                        newLayout += '&lt;skootable showconditions=\"true\" showsavecancel=\"true\" searchmethod=\"server\"'\n                            + ' searchbox=\"true\" showexportbuttons=\"false\" pagesize=\"10\" '\n                            + ' createrecords=\"true\" model=\"' + modelId + '\" mode=\"read\"&gt;'\n                            + '&lt;fields&gt;';\n               